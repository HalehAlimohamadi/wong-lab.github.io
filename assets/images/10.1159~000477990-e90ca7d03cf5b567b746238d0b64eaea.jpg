        var relatedArticlesSlider = undefined;

        function loadDataInline(doiformatted, divId, urlAsync) {
            $(divId).html('');
            $('#TapRelatedContainer').hide();
            $.ajax({
                url: urlAsync,
                data: "doiformatted=" + doiformatted + "&textlength=180&authorsCount=4&titlelength=100",
                success: function (data) {
                    var items = [];
                    var i = 0;
                    items.push('<div class="relatedArticlesSlider">');
                    $.each(data, function (key, val) {
                        i++;
                        var sliderItem = '<div class="slide ' + val.ArticleType + '" id="' + key + '">';
                        if (val.LogoUrl !== "") {
                            sliderItem += '<a href="/Article/FullText/' + val.ArtikelNummer + '" title="' + val.ArtikelCitation + ' (DOI: 10.1159/' + ('000000000' + val.ArtikelNummer).slice(-9) + ')" onclick="ga(\'send\', \'event\', \'FullText\', \'Select Related Article\', \'Sidebar\');" class="NoArrow"><img src="' + val.LogoUrl + '" alt="Journal Logo" /></a>';
                        } else {
                            sliderItem += '<span class="relatedArticlesPlaceholder"></span>';
                        }
                        sliderItem += '<span class="smallText citation">' + val.ArtikelCitation + '</span>';
                        sliderItem += '<a style="display: inline-block; margin-bottom:5px;" href="/Article/FullText/' + val.ArtikelNummer + '" title="' + val.ArtikelCitation + ' (DOI: 10.1159/' + ('000000000' + val.ArtikelNummer).slice(-9) + ')" onclick="ga(\'send\', \'event\', \'FullText\', \'Select Related Article\', \'Sidebar\');"><strong>' + val.ArtikelTitel + '</strong></a>';
                        sliderItem += '<span class="smallText">' + val.Authors + '</span>';
                        sliderItem += '<span>' + val.Abstract + '</span>';
                        sliderItem += '<a class="goToLink" href="/Article/FullText/' + val.ArtikelNummer + '" title="' + val.ArtikelCitation + ' (DOI: 10.1159/' + ('000000000' + val.ArtikelNummer).slice(-9) + ')" onclick="ga(\'send\', \'event\', \'FullText\', \'Select Related Article\', \'Sidebar\');"><strong>Go to Article</strong></a>';
                        sliderItem += '</div>';
                        items.push(sliderItem);
                    });
                    items.push('</div><br/>');
                    if (i > 0) {
                        $('#TapRelatedContainer').show();
                        $('<span/>', {
                            html: items.join('')
                        }).appendTo($(divId));
                        if ($(document).width() <= 500) {
                            loadSlider(1.7, 1.0);
                        } else {
                            loadSlider(2.4, 1.4);
                        }
                    }
                }
            });
        }

        function loadSlider(maxSlides, moveSlides) {
            if (relatedArticlesSlider !== undefined) {
                relatedArticlesSlider.destroySlider();
            }

            relatedArticlesSlider = $('.relatedArticlesSlider').bxSlider({
                pager: false,
                slideWidth: 240,
                minSlides: maxSlides,
                maxSlides: maxSlides,
                moveSlides: moveSlides,
                infiniteLoop: false,
                slideMargin: 10,
                adaptiveHeight: false,
                preventDefaultSwipeY: false,
                oneToOneTouch: false,
                nextSelector: '#relatedArticlesNext',
                prevSelector: '#relatedArticlesPrev',
                nextText: '<i class="fas fa-chevron-right"></i>',
                prevText: '<i class="fas fa-chevron-left"></i>',
                hideControlOnEnd: true,
                onSliderResize: function (currentIndex) {
                    if ($(document).width() <= 500) {
                        loadSlider(1.7, 1.0);
                    } else {
                        loadSlider(2.4, 1.4);
                    }
                },
                onSlideAfter: function (slideElement, oldIndex, newIndex) {
                    //console.log(oldIndex + " - " + newIndex);
                }
            });
        }
