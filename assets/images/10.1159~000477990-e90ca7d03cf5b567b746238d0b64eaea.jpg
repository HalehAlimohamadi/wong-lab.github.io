    <!-- SignUp Modal -->
    <div class="modal fade" tabindex="-1" role="dialog" id="modal_SignUp" style="display: none;">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-body" id="modal_SignUp-Process">

                    <div class="modal_Image"></div>

                    <div class="modal_Steps">

                        <img src="/Images/kargerlogo.gif" alt="karger Logo" class="pull-left" id="kargerlogo" />
                        <button type="button" class="close pull-right" aria-label="Close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                        <div class="clear"></div>


                        <!-- Set email for new account -->
                        <div id="modal_SignUp-SetAccountEmail" class="modal_SignUp-Step">
                            <h2>Sign up for MyKarger</h2>

                            <form onsubmit="return false">
                                <input type="hidden" id="gcmptoken_SignUp_SetAccountEmail" name="gcmptoken_SignUp_SetAccountEmail" value="" />

                                <div class="wrapper-progressBar mb-20">
                                    <ul class="progressBar">
                                        <li id="step1" class="active" title="Set E-mail address"> </li>
                                        <li id="step2a"> </li>
                                        <li id="step3"> </li>
                                    </ul>
                                </div>
                                <div class="clear"></div>

                                <p class="mt-20 mb-20">
                                    Manage with MyKarger your orders simply and fast, save your favorite articles in your reading list, edit your newsletter profile and benefit from attractive discounts.
                                </p>

                                <div class="form-group">
                                    <label for="signUp_emailaddress">E-mail address</label>
                                    <input type="text" id="signUp_emailaddress" class="form-control signUp_emailaddress" placeholder="example@mail.com" autofocus />
                                    <span class="small text-danger" id="signUp_emailaddress_error"></span>
                                </div>

                                <div class="right mt-20 mb-20">
                                    <button type="button" class="btn btn-karger" id="signUp_SetAccountEmail_Submit" onclick="signUp_startprocess(); GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '1', 'Button Click Next');" disabled>next</button>
                                </div>

                                <p class="small pt-20">
                                    This site is protected by reCAPTCHA and the Google <a href="https://policies.google.com/privacy">Privacy Policy</a> and <a href="https://policies.google.com/terms">Terms of Service</a> apply.
                                </p>

                            </form>
                        </div>


                        <!-- Set password for new account -->
                        <div id="modal_SignUp-SetPassword" class="modal_SignUp-Step dNone">
                            <h2>Set Your Password</h2>

                            <form onsubmit="return false">
                                <div class="wrapper-progressBar mb-20">
                                    <ul class="progressBar">
                                        <li id="step1" class="done" onclick="signUp_goToStep('SetAccountEmail', 'backward')" title="Set E-mail address"> </li>
                                        <li id="step2a" class="active" title="Set Password"> </li>
                                        <li id="step3"> </li>
                                    </ul>
                                </div>
                                <div class="clear"></div>

                                <p class="mt-20 mb-20">
                                    Your password must comply with the following rules:

                                    <ul class="ul bold passwordChecker">
                                        <li class="req req1">One number <span class="visible-desktop">(0-9)</span></li>
                                        <li class="req req2">One special character (e.g. ~^*_=+)</li>
                                        <li class="req req3">Minimum 8 characters</li>
                                    </ul>
                                </p>

                                <div class="form-group mt-20">
                                    <span class="signUp_togglePW pull-right"><i class="far fa-eye-slash" aria-hidden="true" title="Show/Hide Password"></i> <span class="small pw-text">Hide password</span></span>
                                    <label for="signUp_newPassword">Password</label>
                                    <input type="text" id="signUp_newPassword" name="signUp_newPassword" class="form-control signUp_password" placeholder="Password" />
                                </div>

                                <div class="right mt-20">
                                    <button type="button" class="btn btn-karger" id="setPassword_Submit" onclick="signUp_goToStep('CompleteName'); GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '2', 'Button Click Next'); GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '3', 'Start Name');" disabled>Next</button>
                                </div>

                            </form>
                        </div>


                        <!-- User already exists: Login -->
                        <div id="modal_SignUp-UserExists" class="modal_SignUp-Step dNone">
                            <h2>Log in to MyKarger?</h2>

                            <form method="post" novalidate="novalidate" id="signUp_ExistingUser" onsubmit="return false">
                                <input id="ReturnURL" name="ReturnURL" value="/Article/Abstract/481584" type="hidden">

                                <div class="wrapper-progressBar mb-20">
                                    <ul class="progressBar">
                                        <li id="step1" class="done" onclick="signUp_goToStep('SetAccountEmail', 'backward')" title="Set E-mail address"> </li>
                                        <li id="step2b" class="active" title="Login with existing Password"> </li>
                                    </ul>
                                </div>
                                <div class="clear"></div>

                                <p class="mt-20 mb-20">
                                    We found an already existing MyKarger account with this e-mail address: <span class="bold" id="givenMailAddress"></span>
                                </p>

                                <div class="form-group mt-20">
                                    <span class="signUp_togglePW pull-right"><i class="far fa-eye-slash" aria-hidden="true" title="Show/Hide Password"></i> <span class="small pw-text">Hide password</span></span>
                                    <label for="signUp_existingPassword">Password</label>
                                    <input type="text" id="signUp_existingPassword" name="signUp_existingPassword" class="form-control signUp_password" placeholder="Password" />
                                    <span class="small text-danger dNone" id="signUp_existingPasswordIncorrect"></span>
                                </div>

                                <div class="form-group mt-20 pull-left">
                                    <input data-val="true" id="signUp_RememberMe" name="signUp_RememberMe" type="checkbox" value="true" class="pull-left">
                                    <input name="signUp_RememberMe" type="hidden" value="false">
                                    <label for="signUp_RememberMe" class="form-control ml-20">Keep me logged in</label>
                                </div>
                                <div class="pull-right right mt-20">
                                    <button type="button" class="btn btn-karger" id="btn_UserExists-SignIn" onclick="signIn_ExistingUser(); GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '2', 'Button Click Login');" disabled>Login</button><br />
                                    <button type="button" class="btn btn-passiv mt-10 forgotPasswordLink" id="btn_UserExists-ForgotPW" onClick="GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '2', 'Button Click Forgot Password');">Forgot password</button>
                                </div>
                                <div class="clear"></div>

                            </form>
                        </div>


                        <!-- Complete your name and accept Terms & Conditions -->
                        <div id="modal_SignUp-CompleteName" class="modal_SignUp-Step dNone">
                            <h2>Complete Your Name</h2>

                            <form onsubmit="return false">
                                <input type="hidden" id="gcmptoken_SignUp_CompleteName" name="gcmptoken_SignUp_CompleteName" value="" />
                                <input type="hidden" id="signUp_returnUrl" name="signUp_returnUrl" value="" />

                                <div class="wrapper-progressBar mb-20">
                                    <ul class="progressBar">
                                        <li id="step1" class="done" onclick="signUp_goToStep('SetAccountEmail', 'backward')" title="Set E-mail address"> </li>
                                        <li id="step2" class="done" onclick="signUp_goToStep('SetPassword')" title="Set Password"> </li>
                                        <li id="step3" class="active"> </li>
                                    </ul>
                                </div>
                                <div class="clear"></div>

                                <div class="form-group mt-20">
                                    <label for="signUp_firstname">First Name</label>
                                    <input type="text" id="signUp_firstname" class="form-control" placeholder="First Name" />
                                </div>

                                <div class="form-group mt-20">
                                    <label for="signUp_firstname">Last Name</label>
                                    <input type="text" id="signUp_lastname" class="form-control" placeholder="Last Name" />
                                </div>

                                <div class="form-group mt-20">
                                    <input type="checkbox" id="signUp_TermsAndConditions" Class="pull-left" />
                                    <label for="signUp_TermsAndConditions" class="form-control ml-20">I have read the Karger <a href="/Info/Terms" target="_blank">Terms and Conditions</a> and agree.</label>
                                </div>

                                <div id="signUp_createAccount_error" class="small text-danger"></div>

                                <div class="right mt-20">
                                    <button type="button" class="btn btn-karger" id="signUp_createAccount" onclick="signUp_CreateAccount(); GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '3', 'Button Click Create Account');" disabled>create account</button>
                                </div>

                            </form>
                        </div>


                        <!-- Account created -->
                        <div id="modal_SignUp-AccountCreated" Class="modal_SignUp-Step dNone">
                            <h2>Thank You For Registering</h2>

                            <p class="mt-20 mb-20" style="display: flex; align-items: center;">
                                <span class="fa-stack fa-lg mr-10">
                                    <i class="fas fa-circle fa-stack-2x"></i>
                                    <i class="fas fa-check fa-stack-1x fa-inverse"></i>
                                </span>

                                Your MyKarger account has been created.
                            </p>

                            <p class="bold mb-20">
                                Please check your e-mails to validate your e-mail address.
                            </p>

                            <div class="mt-20">
                                <button type="button" class="btn btn-karger" id="signUp_btnDone" data-dismiss="modal" onclick="GTMDatalayerSignUpMyKarger('Sign Up MyKarger', '4', 'Button Click Done');">done</button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- ResetPassword Modal -->
    <div class="modal fade" tabindex="-1" role="dialog" id="modal_ResetPassword" style="display: none;">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-body" id="modal_ResetPassword-Process">

                    <div class="modal_Image"></div>

                    <div class="modal_Steps">

                        <img src="/Images/kargerlogo.gif" alt="karger Logo" class="pull-left" id="kargerlogo" />
