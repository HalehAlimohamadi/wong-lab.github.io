                        <div class="clear"></div>

                        <!-- RESET PASSWORD -->
                        <div id="modal_ResetPassword-Request" class="modal_SignUp-Step modal_ResetPW-Step">
                            <h2>Reset Your Password</h2>
                            <form id="resetPassword" method="post" novalidate="novalidate" onsubmit="return false">

                                <p class="mb-20">
                                    To reset your password, enter your e-mail address or your user ID you registered with. You will be sent an e-mail containing a link to reset your password.
                                </p>

                                <div class="form-group">
                                    <label for="signUp_resetEmailaddress">E-mail address / User ID</label>
                                    <input type="text" id="signUp_resetEmailaddress" class="form-control" placeholder="example@mail.com" />
                                    <span class="small text-danger dNone" id="signUp_resetEmailaddress_error">Something went wrong</span>
                                </div>

                                <div class="right mt-20">
                                    <button type="button" id="resetPassword_Submit" class="btn btn-karger" onclick="resetPassword_submitMailOrID(); GTMDatalayerSignUpMyKarger('Reset Password', '1', 'Button Click Submit');">Submit</button>
                                </div>
                            </form>
                        </div>

                        <div id="modal_ResetPassword-Confirm" class="modal_SignUp-Step modal_ResetPW-Step dNone">
                            <h2>Check Your E-Mails</h2>

                            <form>
                                <p class="mb-20">
                                    A link to reset your password has been sent to your e-mail address. Follow the instructions and try to log in again.
                                </p>

                                <div class="mt-20">
                                    <button type="button" class="btn btn-karger" data-dismiss="modal" onclick="GTMDatalayerSignUpMyKarger('Reset Password', '2', 'Button Click Close');">Close</button>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>




    



